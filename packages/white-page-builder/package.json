{
  "name": "@globalmarketinggroup/white-page-builder",
  "version": "1.0.1",
  "scripts": {
    "dev": "yarn dev",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "yarn prod",
    "production": "mix --production",
    "serve": "php -S localhost:8000"
  },
  "standard": {
    "envs": {
      "browser": true,
      "jquery": true
    }
  },
  "browserslist": [
    "> 5%",
    "last 4 versions",
    "ie >= 10",
    "Firefox ESR",
    "Safari >= 10"
  ],
  "files": [
    "dist"
  ],
  "style": "dist/css/pageBuilder.css",
  "main": "dist/pageBuilder.js",
  "license": "MIT",
  "devDependencies": {
    "resolve-url-loader": "^4.0.0",
    "sanitize.css": "^11.0.0",
    "tinymce": "^5.0.14"
  }
}
