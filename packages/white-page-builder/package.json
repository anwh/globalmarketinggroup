{
  "name": "@koterion/page_builder",
  "version": "1.2.2",
  "description": "Plugin for create page structure",
  "scripts": {
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production"
  },
  "standard": {
    "envs": {
      "browser": true,
      "jquery": true
    }
  },
  "browserslist": [
    "> 5%",
    "last 4 versions",
    "ie >= 10",
    "Firefox ESR",
    "Safari >= 10"
  ],
  "files": [
    "dist"
  ],
  "style": "dist/css/pageBuilder.css",
  "main": "dist/pageBuilder.js",
  "author": "koterion",
  "license": "MIT",
  "devDependencies": {
    "resolve-url-loader": "^4.0.0",
    "sanitize.css": "^11.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.1.0",
    "tinymce": "^5.0.14"
  }
}
